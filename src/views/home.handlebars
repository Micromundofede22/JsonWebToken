<div>
    <table>
        <thead>
            <tr>
                <th>Titulo</th>
                <th>Descripción</th>
                <th>Precio</th>
                <th>Disponibilidad</th>
                <th>Stock</th>
                <th>Categoría</th>
                <th>Fotografía</th>
                <th>Comprar</th>
            </tr>
        </thead>
        <tbody>
            {{#each docs}}
            <tr>
                <td>{{this.title}}</td>
                <td>{{this.description}}</td>
                <td>{{this.price}}</td>
                <td>{{this.status}}</td>
                <td>{{this.stock}}</td>
                <td>{{this.category}}</td>
                <td>{{this.thumbnails}}</td>
                <td><a href="{{addLink}}">Add</a></td>
            </tr>
            {{/each}}
            <tr>

            </tr>
        </tbody>
    </table>

<div>
    {{#if hasPrevPage}}
    <a href="{{prevLink}}">Anterior</a>
    {{/if}}
    {{#if hasNextPage}}
    <a href="{{nextLink}}">Siguiente</a>
    {{/if}}
</div>

</div>


